{
  "name": "gulp-squoosh",
  "version": "0.3.1",
  "description": "Gulp plugin to compress images using Squoosh",
  "keywords": [
    "gulpplugin",
    "squoosh",
    "image",
    "compress",
    "convert",
    "minify",
    "png",
    "jpg",
    "jpeg",
    "mozjpeg",
    "oxipng",
    "webp",
    "avif",
    "wp2",
    "jxl"
  ],
  "author": "Eugene <mail2chenaski@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenaski/gulp-squoosh.git"
  },
  "bugs": {
    "url": "https://github.com/chenaski/gulp-squoosh/issues"
  },
  "homepage": "https://github.com/chenaski/gulp-squoosh",
  "engines": {
    "node": " ^12.5.0 || ^14.0.0 || ^16.0.0 "
  },
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "!lib/*.spec.js"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "prettier -wu"
  },
  "scripts": {
    "format": "prettier -wu .",
    "test": "jest --testPathIgnorePatterns tests/hard",
    "test:hard": "jest --testPathPattern tests/hard",
    "test:docker": "zx tests/run.js",
    "test:docker:serial": "tests/run"
  },
  "dependencies": {
    "@squoosh/lib": "^0.3.1",
    "plugin-error": "^1.0.1",
    "through2": "^2.0.5"
  },
  "devDependencies": {
    "get-stream": "^6.0.1",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "simple-git-hooks": "^2.4.1",
    "vinyl": "^2.2.1",
    "zx": "^1.14.2"
  }
}
